	@echo "#endif // !defined(PPL_CREDITS_hh)" >>$@
CREDITS.cc: $(top_builddir)/utils/text2cxxarray $(top_srcdir)/CREDITS CREDITS.hh
	@echo "#include \"CREDITS.hh\"" >$@
	$(top_builddir)/utils/text2cxxarray --name=CREDITS_array \
		$(top_srcdir)/CREDITS >>$@

